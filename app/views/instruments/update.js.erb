<% @instrument.attributes.each do |name, value| %>
  <% if Instrument.metrics_attributes.include?(name) %>
    $("#<%= name %>_status_instrument_<%= @instrument.id %>").html(
      "<%=
        j render partial: 'shared/metrics_icon',
          locals: {
          record: @instrument,
          name: name,
          value: value
        }
      %>");
  <% end %>
<% end %>

