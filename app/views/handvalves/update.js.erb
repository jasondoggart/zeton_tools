<% @handvalve.attributes.each do |name, value| %>
  <% if Handvalve.metrics_attributes.include?(name) %>
    $("#<%= name %>_status_handvalve_<%= @handvalve.id %>").html(
      "<%=
        j render partial: 'shared/metrics_icon',
          locals: {
          record: @handvalve,
          name: name,
          value: value
        }
      %>");
  <% end %>
<% end %>
$("#handvalve-properties").html("<%= j render 'properties' %>");
